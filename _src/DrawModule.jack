        let LEVEL_TEXT_COL          =   0;  
        let MODE_TEXT_LINE          =  22;
        let MODE_TEXT_COL           =   2;
        let MEMO_TEXT_LINE          =  22;
        let MEMO_TEXT_COL           =  34;
        let MEMO_DISP_OFFSET_TOP    = 243;
